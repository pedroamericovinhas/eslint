{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "no-extra-semi": ["error"],
      "comma-dangle": [
          "error",
          "always-multiline"
      ],
      "no-var": 1,
      "array-bracket-newline": [
          "error", 
          {"multiline": true}
      ],
      "indent": [
          "error",
          2,
          {"SwitchCase": 1}
      ],
      "quotes": [
          "error",
          "double",
          {"allowTemplateLiterals": true}
      ],
      "space-in-parens": [
          "error", "never"
      ],
      "prefer-template": [
          "error"
      ],
      "no-undef": 0,
      "no-unused-vars": 0,
      "object-curly-spacing": [
          "error", "never"
      ],
      "template-curly-spacing": [
          "error", "never"
      ]
  }
}
